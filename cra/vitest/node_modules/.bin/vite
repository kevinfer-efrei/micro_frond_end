#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/bin/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/bin/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@5.0.12_@types+node@20.9.0/node_modules/vite/bin/vite.js" "$@"
fi
