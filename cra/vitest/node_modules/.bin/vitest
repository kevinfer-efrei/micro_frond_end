#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules/vitest/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules/vitest/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules:/Users/pauld/development/micro-frontend/micro_front_end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitest@1.2.2_@types+node@20.9.0/node_modules/vitest/vitest.mjs" "$@"
fi
